{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": "eslint:recommended",
  "globals": {
    "__dirname": false
  },
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "no-control-regex": "off",
    "no-empty": "warn",
    "no-extra-boolean-cast": "off",
    "no-extra-semi": "warn",
    "no-irregular-whitespace": "warn",
    "no-mixed-spaces-and-tabs": "warn",
    "no-prototype-builtins": "off",
    "no-redeclare": "off",
    "no-unreachable": "warn",
    "no-unused-vars": "warn",
    "no-useless-escape": "off"
  }
}
